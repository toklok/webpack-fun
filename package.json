    "bundle-buddy-webpack-plugin": "^0.3.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-last": "^0.0.3",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-prettier": "2.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
